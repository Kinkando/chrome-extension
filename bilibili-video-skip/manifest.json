{
    "manifest_version": 3,
    "name": "BiliBili Video Skipper",
    "version": "0.0.0.4",
    "description": "Chrome extension for auto skip any ads video on BiliBili",
    "author": "Kinkando",
    "icons": {
        "16": "assets/icons/16x16.png",
        "48": "assets/icons/48x48.png",
        "128": "assets/icons/128x128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://www.bilibili.tv/*"],
            "js": ["script.js"]
        }
    ]
}